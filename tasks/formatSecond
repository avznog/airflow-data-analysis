from pyspark.sql import SparkSession

def format_second():
	spark = SparkSession.builder \
		.appName("Python Spark SQL basic example") \
		.config("spark.some.config.option", "some-value") \
		.config("spark.jars", "/Users/quentindelaforestdivonne/Desktop/Projet_BDD/dags/lib/postgresql-42.6.0.jar") \
		.getOrCreate()

	df = spark.read \
		.format("jdbc") \
		.option("url", "jdbc:postgresql://bdd.juniorisep.com:54323/quentin1") \
		.option("dbtable", "data_consommation") \
		.option("user", "postgres") \
		.option("password", "postgres") \
		.option("driver", "org.postgresql.Driver") \
		.load()